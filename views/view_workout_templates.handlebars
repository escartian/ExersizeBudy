<h1>Workout Templates</h1>

<ul>
  {{#each workouts}}
  <li>
    <h2>{{this.name}}</h2>
    <h3>Exercises:</h3>
    <ul>
      {{#each this.exercises}}
      <li>
        <h4>{{this.exerciseName}}</h4>
        <p>Sets: {{this.sets}}</p>
        <p>Reps: {{this.reps}}</p>
        <p>Weight: {{this.weight}}</p>
      </li>
      {{/each}}
    </ul>
  </li>
  {{/each}}
</ul>